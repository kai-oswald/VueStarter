{
  "name": "vue-starter",
  "description": "Simple ASP.Net Core Vue starter",
  "author": "KO",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "cross-env ASPNETCORE_ENVIRONMENT=Development NODE_ENV=development dotnet run",
    "build": "npm run build-vendor:prod && npm run build:prod",
    "build:dev": "webpack --hide-modules",
    "build:prod": "npm rebuild node-sass && webpack --config webpack.config.prod.js --hide-modules",
    "build-vendor:prod": "cross-env NODE_ENV=production webpack --config webpack.config.vendor.js",
    "build-vendor:dev": "cross-env NODE_ENV=development webpack --config webpack.config.vendor.js",
    "install": "npm run build-vendor:dev"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "core-js": "^2.5.7",
    "vue": "^2.5.16",
    "vue-router": "^2.8.1"
  },
  "devDependencies": {
    "aspnet-webpack": "^2.0.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "cross-env": "^3.2.4",
    "css-loader": "^1.0.0",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "latest",
    "event-source-polyfill": "0.0.7",
    "file-loader": "^0.9.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.13.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.1.1",
    "vue-loader": "^14.2.3",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.22.3"
  }
}
